# JSR305

maven_jar(
    name = "jsr305_maven",
    artifact = "com.google.code.findbugs:jsr305:3.0.2",
)

bind(
    name = "jsr305",
    actual = "@jsr305_maven//jar"
)

# JUnit
maven_jar(
    name = "junit_maven",
    artifact = "junit:junit:4.12",
)

bind(
    name = "junit",
    actual = "@junit_maven//jar"
)

# Hamcrest
maven_jar(
    name = "hamcrest_maven",
    artifact = "org.hamcrest:java-hamcrest:2.0.0.0",
)

bind(
    name = "hamcrest",
    actual = "@hamcrest_maven//jar"
)
